<h1>Artists#index</h1>
<p>Find me in app/views/artists/index.html.erb</p>
<%= link_to "New Artist", new_artist_path(@artist) %>
<% @artists.each do |artist| %>
  <h2> <%= artist.name %> </h2>
  <%= link_to "View #{artist.name}", artists_path(artist)%>
  <br>
  <%= link_to "Edit #{artist.name}", edit_artist_path(artist) %>
  <br>
  <%= link_to "Delete #{artist.name}", @artist, method: :Delete%>
<%end%>


<h1>Artists</h1>
<%= link_to "New Artist", new_artist_path(@artist) %>
<%= link_to "View Billboard 100", song_billboards_path(@billboard)%>

<% @artists.each do |artist| %>
  <h2> <%= artist.name %> </h2>
  <%= link_to "View #{artist.name} songs", artist_songs_path(artist)%>
  <br>
  <%= link_to "Edit #{artist.name}", edit_artist_path(artist) %>
  <br>
  <%= link_to "Delete #{artist.name}", @artist, method: :Delete%>
<%end%>

